package Ejercicios;

import java.util.Scanner;

public class Ejercicio2Array {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println(" Indica un numero");
		int i = sc.nextInt();
		Integer[] num = new Integer[6];
		int contador = ((i * i) + 1);
		for (i = 0; i < contador; i++) {
			num[1] = contador;
			System.out.println();
		}
		sc.close();
	}

}
