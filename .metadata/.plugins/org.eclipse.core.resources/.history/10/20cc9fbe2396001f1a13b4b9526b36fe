package ejercicio;

import java.util.Scanner;

public class ejercicio1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner scanner = new Scanner(System.in);
		System.out.println("¿cual es tu año de nacimiento?");

		Integer añoNacimiento = scanner.nextInt();

		if (añoNacimiento >= 1883 && añoNacimiento <= 1900) {
			System.out.println("Generacion Perdida");
		} else if (añoNacimiento >= 1901 && añoNacimiento <= 1927) {
			System.out.println("Generacion Grandiosa ");
		}

		else if (añoNacimiento >= 1928 && añoNacimiento <= 1945) {
			System.out.println(" Generacion Silenciosa ");

		} else if (añoNacimiento >= 1946 && añoNacimiento <= 1964) {
			System.out.println("Baby Boomers ");
		}

		else if (añoNacimiento >= 1965 && añoNacimiento <= 1980) {
			System.out.println("Generacion X ");
		}

		else if (añoNacimiento >= 1981 && añoNacimiento <= 1996) {
			System.out.println("Generacion Y ");
		}

		else if (añoNacimiento >= 1997 && añoNacimiento <= 2012) {
			System.out.println("Generacion Z ");
		}

		else if (añoNacimiento >= 2012 && añoNacimiento <= 2020) {
			System.out.println("Generacion Alfa ");
		}
		scanner.close();

	}

}