package ejercicio;

import java.util.Scanner;

public class ejercicio5repaso {


	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		
int resultadoSuma = 0; // acumulado las sumas parciales
int contador = 1;
while(contador <= 10000) {
	resultadoSuma += contador; // aqui le decimos que vaya sumando al contador el nuevo numero
	contador ++; // contador = contador + 1 (tambien se puedo poner de esta forma)

	
	}
System.out.println("La suma de todos los nuemros del el 1  al 10000 es " );

	}

	
	
}
