package ejercicio;

import java.util.Scanner;

public class ejercici12basico {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		Integer n =0;
		System.out.println("introduce un numero");
		n = sc.nextInt();
		 Integer i = 0;
		if (n >0) {
			while ( i < n) {
				i ++;
				System.out.print(" ! ");
				
			}
		}

	}

}
