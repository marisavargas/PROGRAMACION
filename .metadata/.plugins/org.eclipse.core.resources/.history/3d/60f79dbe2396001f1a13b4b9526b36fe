package ejercicio;

public class ejercicio1repasobasico {

	public static void main(String[] args) {
	int x = 144;
	int y = 999;
	int suma = x + y;
	int resta = x - y;
	int multiplicacion = x * y;
	int division = x / y;
	
System.out.println(suma);
System.out.println(resta);
System.out.println(multiplicacion);
System.out.println(division);
	}

}


//Escribe un programa en el que se declaren las variables enteras x e y. Asígnales los valores 144 y 999
//respectivamente. A continuación, muestra por pantalla el valor de cada variable, la suma, la resta, la
//división y la multiplicación