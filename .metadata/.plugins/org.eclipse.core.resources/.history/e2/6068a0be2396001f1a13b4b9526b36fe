package ejercicio;

public class ejercicio5repasoclase {

	public static void main(String[] args) {

		for (int numero = 0; numero <= 20; numero++) {
			if ( numero == 20) {
				System.out.print( numero);
			}
			else {	System.out.print(numero + "_"); // escribo que es >=20 porque quiero que me de los numeros hasta 20
			}
				
			 
		}
		
// ESTUDIAR
	}

}
